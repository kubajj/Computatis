        \KeywordTok{this}\NormalTok{.}\VariableTok{$data}\NormalTok{.}\AttributeTok{resultsInputs}\NormalTok{[i] }\OperatorTok{=} \StringTok{''}\OperatorTok{;}
    \OperatorTok{\}}
